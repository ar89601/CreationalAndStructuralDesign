package singleton;

public class UserNameRegistration {
	//single instance
	private static final UserNameRegistration uniqueUserName = new UserNameRegistration();

	//private constructor
	private UserNameRegistration(){}
	
	//get instance
	public static UserNameRegistration getUserName(){
		return uniqueUserName;
	}
}



