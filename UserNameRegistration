package singleton;

public class UserNameRegistration {
	//single instance
	private static final UserNameRegistration UserName = new UserNameRegistration();

	//private constructor
	private UserNameRegistration(){}
	
	//get instance
	public static UserNameRegistration getSingeltonOfUserName(){
		return UserName;
	}
	
	protected static void uniqueSingleton(){
		System.out.println("User name accepted. Please enter a password.");
	}
}



