package decorator;

public class Tomato extends Veggies{
	//constructor
	public Tomato(Condiment c){
		super(c);
	}
	
	// specific name method for tomato
	@Override
	public void name(){
		System.out.println("You have selected tomato. Type 'c' to continue.");
	}
}
