package decorator;

abstract class CondimentDecorator implements Condiment {

	protected Condiment condiment;

	// constructor
	public CondimentDecorator(Condiment c) {
		this.condiment = c;
	}

	// specific name method for condiment decorator
	@Override
	public void name() {
		this.condiment.name();
	}
}
